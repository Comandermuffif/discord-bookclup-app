{
  "name": "discord-bookclup-app",
  "version": "1.0.0",
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "tsx watch --inspect src/index.ts",
